{
  "version": 2,
  "functions": {
    "server/api/index.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/api/index.ts"
    },
    {
      "source": "/health",
      "destination": "/server/api/index.ts"
    }
  ]
}

